@model GeoHelper.Models.UsersProjects

@{
    ViewData["Title"] = "Delete";
}

<h2>Usuń kolaboranta</h2>

<h3>Jesteś pewny, że chcesz usunąć tego kolaboranta??</h3>
<div>
    <h4>Kolaborant w projekcie</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.projectId)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.projectId)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.user)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.user)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ID" />
        <input type="submit" value="Usuń" class="btn btn-default" /> |
        <a asp-action="Index" asp-route-id="@Model.projectId">Powrót do listy kolaborantów</a>
    </form>
</div>
